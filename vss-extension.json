{
    "manifestVersion": 1,
    "id": "azdevopsSFTP",
    "publisher": "adacta",
    "version": "1.1.79",
    "name": "SFTP / SSH download",
    "description": "SFTP / SSH file Download task for Azure DevOps build and release pipeline.",
    "public": true,
    "categories": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "SFTP",
        "SSH",
        "deploy"
    ],
    "screenshots": [
        {
            "path": "screenshots/SFTP-Download-hostname.png"
        },
        {
            "path": "screenshots/SFTP-Download-task.png"
        }
    ],
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "icons": {
        "default": "images/sftp-icon.png"
    },
    "contributions": [
        {
            "id": "task-sftp-download",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.task"
            ],
            "properties": {
                "name": "sftpDownload"
            }
        }
    ],
    "files": [
        {
            "path": "sftpDownload"
        },
        {
            "path": "screenshots",
            "addressable": true
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://adaxtools.visualstudio.com/_git/Azure%20DevOps%20-%20SFTP"
    }
}